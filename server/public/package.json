{
  "name": "pronto8-ws",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:libs",
    "start": "ng serve",
    "build:libs:tools:fetch": "ng build fetch",
    "build:libs:tools:core": "ng build core",
    "build:libs:tools:auth-background": "ng build authentication-background",
    "build:libs:tools": "npm run build:libs:tools:core && npm run build:libs:tools:fetch && npm run build:libs:tools:auth-background",
    "build:libs:auth-foreground": "ng build authentication-foreground",
    "build:libs:mail": "ng build mail",
    "build:libs:settings": "ng build settings",
    "build:libs:search": "ng build search",
    "build:libs": "npm run build:libs:tools && npm run build:libs:auth-foreground && npm run build:libs:mail && npm run build:libs:settings && npm run build:libs:search",
    "build:app": "ng build main",
    "build:app:prod": "ng build main --configuration production && ng build core --configuration production && ng build fetch --configuration production && ng build authentication-foreground --configuration production && ng build authentication-background --configuration production && ng build mail --configuration production && ng build settings --configuration production",
    "watch:libs:core": "ng build core --watch --configuration development",
    "watch:libs:fetch": "ng build fetch --watch --configuration development",
    "watch:libs:auth-foreground": "ng build authentication-foreground --watch --configuration development",
    "watch:libs:auth-background": "ng build authentication-background --watch --configuration development",
    "watch:libs:mail": "ng build mail --watch --configuration development",
    "watch:libs:settings": "ng build settings --watch --configuration development",
    "watch:libs:search": "ng build search --watch --configuration development",
    "watch:app": "ng build main --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.1",
    "@angular/cdk": "^17.3.1",
    "@angular/common": "^17.3.1",
    "@angular/compiler": "^17.3.1",
    "@angular/core": "^17.3.1",
    "@angular/forms": "^17.3.1",
    "@angular/material": "^17.3.1",
    "@angular/platform-browser": "^17.3.1",
    "@angular/platform-browser-dynamic": "^17.3.1",
    "@angular/router": "^17.3.1",
    "@ngrx/signals": "^17.1.1",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "rxjs": "~7.8.0",
    "simple-xml-to-json": "^1.2.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.1",
    "@angular/cli": "^17.3.1",
    "@angular/compiler-cli": "^17.3.1",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.3.2"
  }
}